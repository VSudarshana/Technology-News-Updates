{
    "name": "Technology News Updates",
    "version": "1.0",
    "description": "A Chrome extension that displays technology news updates.",
    "manifest_version": 3,
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["*://*/*"],
        "js": ["contentscript.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  } 
  
  